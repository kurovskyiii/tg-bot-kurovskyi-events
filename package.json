{
  "name": "telegram-bot-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Kurovskyi Channel Bot",
  "author": "Kurovskyi <me@kurovskyi.dev>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "tsc-watch --onSuccess \"tsx ./src/main.ts\"",
    "start": "tsc && tsx ./src/main.ts",
    "start:force": "tsx ./src/main.ts",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@commonify/lowdb": "^3.0.0",
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/chat-members": "^1.1.0",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/menu": "^1.2.2",
    "@grammyjs/parse-mode": "1.10.0",
    "@grammyjs/ratelimiter": "^1.2.0",
    "@grammyjs/runner": "2.0.3",
    "@grammyjs/storage-file": "^2.4.2",
    "@grammyjs/storage-free": "^2.4.2",
    "@grammyjs/types": "3.11.1",
    "@hono/node-server": "1.13.1",
    "callback-data": "1.1.1",
    "grammy": "1.27.0",
    "grammy-guard": "^0.5.0",
    "grammy-scenes": "^10.2.0",
    "hono": "4.6.4",
    "install": "^0.13.0",
    "iso-639-1": "3.1.3",
    "lowdb": "^7.0.1",
    "mkdirp": "^3.0.1",
    "npm": "^10.8.3",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "tsx": "4.19.2",
    "valibot": "0.42.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.26.0",
    "@types/node": "^20.14.12",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.ts": "eslint"
  }
}
